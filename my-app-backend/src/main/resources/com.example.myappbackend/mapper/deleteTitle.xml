<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.myappbackend.mapper.TitleMapper">
    <select id="deleteTitle" parameterType="java.lang.Long">
        UPDATE
            totaltitle
        SET
            active = 0
        WHERE
            id = #{id}
    </select>
</mapper>