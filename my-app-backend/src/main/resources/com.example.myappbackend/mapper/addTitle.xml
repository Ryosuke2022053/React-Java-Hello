<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.myappbackend.mapper.TitleMapper">
    <select id="addTitle" resultType="com.example.myappbackend.entity.TitleEntity">
        INSERT INTO totaltitle (
        title, content, img_path, user_id)
        VALUES ( #{title}, #{content}, #{imgPath}, #{userId});
    </select>
</mapper>